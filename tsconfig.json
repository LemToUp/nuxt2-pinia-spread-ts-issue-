{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/debounce",
      "@types/jest",
      "@types/node",
      "@types/showdown",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/sentry",
      "@nuxtjs/vuetify",
      "@pinia/nuxt",
      "vue-plausible"
    ]
  },
  "exclude": [
    "node_modules",
    "e2e",
    ".nuxt",
    ".nuxt-storybook"
  ]
}
